fun main() {
    var listOfNums: MutableList<Int> = mutableListOf<Int>()
    for (i in 1..10) {
        listOfNums.add(i)
    }
    //Вызов функции печати
    printList(listOfNums)
}

//Функция печати списка
fun printList(listOfNums: List<Int>) {
    for (i in listOfNums) {
        println(i)
    }
}
